{
  "name": "araleeat/trustpilot",
  "description": "PHP library for communication with the Trustpilot API",
  "type": "library",
  "homepage": "https://github.com/araleeat/trustpilot",
  "version": "1.0.3",
  "keywords": [
    "Trustpilot",
    "Trustpilot API",
    "PHP"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "araleeat",
      "email": "araleeat@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "guzzlehttp/oauth-subscriber": "^0.3.0"
  },
  "autoload": {
    "psr-4": {
      "Araleeat\\Trustpilot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Araleeat\\Trustpilot\\Tests\\": "test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "codeclimate/php-test-reporter": "^0.4.4"
  }
}
